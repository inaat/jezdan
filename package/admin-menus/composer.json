{
  "name": "inaat/menue",
  "description": "Laravel Menu management",
  "keywords": [
      "laravel",
      "menus",
      "navigation",
      "nav",
      "bootstrap"
  ],
  "license": "MIT",
  "authors": [
      {
          "name": "Inayat Ullah",
          "email": "inayatullahkks@gmail.com"
      }
  ],
  "require": {
      "php": "^8.0",
      "illuminate/support": "^8.0",
      "illuminate/config": "^8.0",
      "illuminate/view": "^8.0",
    "laravellux/html": "^7.1"
  },
  "require-dev": {
      "phpunit/phpunit": "^8.5",
      "mockery/mockery": "~1.0",
      "orchestra/testbench": "^6.2",
      "friendsofphp/php-cs-fixer": "^2.16"
  },
  "autoload": {
      "psr-4": {
          "Inaat\\AdminMenus\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Inaat\\AdminMenus\\Tests\\": "tests/"
      }
  },
  "extra": {
      "laravel": {
          "providers": [
              "Inaat\\AdminMenus\\MenusServiceProvider"
          ],
          "aliases": {
              "Menu": "Inaat\\AdminMenus\\Facades\\Menu"
          }
      }
  },
  "minimum-stability": "stable"
}
